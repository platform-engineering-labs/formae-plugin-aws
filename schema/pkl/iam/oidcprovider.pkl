module aws.iam.oidcprovider

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::IAM::OIDCProvider"

@aws.ResourceHint {
    type = module.type
    identifier = "Arn"
}
open class OIDCProvider extends formae.Resource {

    @aws.FieldHint
    clientIdList: Listing<String>?

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?

    @aws.FieldHint
    thumbprintList: Listing<String>?

    @aws.FieldHint{createOnly = true}
    url: String?
}
