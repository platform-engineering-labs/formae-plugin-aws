module aws.s3.accessgrantslocation

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::S3::AccessGrantsLocation"

@aws.ResourceHint {
    type = module.type
    identifier = "AccessGrantsLocationId"
    discoverable = false
}
open class AccessGrantsLocation extends formae.Resource {

    @aws.FieldHint
    iamRoleArn: (String|formae.Resolvable)?

    @aws.FieldHint
    locationScope: String?

    @aws.FieldHint {
        createOnly = true
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?

}
