module aws.rds.dbshardgroup

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::RDS::DBShardGroup"


open class DBShardGroupResolvable extends formae.Resolvable {
    hidden type = module.type

    hidden dbShardGroupResourceId: DBShardGroupResolvable = (this) {
        property = "DBShardGroupResourceId"
    }
    hidden endpoint: DBShardGroupResolvable = (this) {
        property = "Endpoint"
    }
}


@aws.ResourceHint {
    type = module.type
    identifier = "DBShardGroupIdentifier"
}
open class DBShardGroup extends formae.Resource {
    @aws.FieldHint
    computeRedundancy: Int?

    @aws.FieldHint{createOnly = true
        outputField = "DBClusterIdentifier"
    }
    dbClusterIdentifier: String|formae.Resolvable

    @aws.FieldHint{createOnly = true
        outputField = "DBShardGroupIdentifier"
    }
    dbShardGroupIdentifier: String?

    @aws.FieldHint
    maxACU: Number

    @aws.FieldHint{writeOnly = true}
    minACU: Number?

    @aws.FieldHint{createOnly = true}
    publiclyAccessible: Boolean?

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?

    hidden parent = this

    hidden res: DBShardGroupResolvable = new {
        label = parent.label
        stack = parent.stack?.label
    }
}
