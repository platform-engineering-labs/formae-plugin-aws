module aws.rds.dbproxyendpoint

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::RDS::DBProxyEndpoint"

typealias DBProxyEndpointTargetRole = "READ_WRITE"|"READ_ONLY"

open class DBProxyEndpointResolvable extends formae.Resolvable {
    hidden type = module.type

    hidden dbProxyEndpointArn: DBProxyEndpointResolvable = (this) {
        property = "DBProxyEndpointArn"
    }
    hidden endpoint: DBProxyEndpointResolvable = (this) {
        property = "Endpoint"
    }
    hidden isDefault: DBProxyEndpointResolvable = (this) {
        property = "IsDefault"
    }
    hidden vpcId: DBProxyEndpointResolvable = (this) {
        property = "VpcId"
    }
}


@aws.ResourceHint {
    type = module.type
    identifier = "DBProxyEndpointArn"
}
open class DBProxyEndpoint extends formae.Resource {

    @aws.FieldHint{
        createOnly = true
        outputField = "DBProxyEndpointName"
    }
    dbProxyEndpointName: String(matches(Regex(#"[0-z]*"#)))


    @aws.FieldHint{
        createOnly = true
        outputField = "DBProxyName"
    }
    dbProxyName: String(matches(Regex(#"[0-z]*"#)))


    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?


    @aws.FieldHint
    targetRole: DBProxyEndpointTargetRole?


    @aws.FieldHint
    vpcSecurityGroupIds: (Listing<String|formae.Resolvable>)?


    @aws.FieldHint{createOnly = true}
    vpcSubnetIds: Listing<String|formae.Resolvable>

    hidden parent = this

    hidden res: DBProxyEndpointResolvable = new {
        label = parent.label
        stack = parent.stack?.label
    }
}