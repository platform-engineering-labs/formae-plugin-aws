module aws.rds.dbsubnetgroup

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::RDS::DBSubnetGroup"

open class DBSubnetGroupResolvable extends formae.Resolvable {
    type = module.type

    hidden dbSubnetGroupDescription: DBSubnetGroupResolvable = (this) {
        property = "DBSubnetGroupDescription"
    }
    hidden dbSubnetGroupName: DBSubnetGroupResolvable = (this) {
        property = "DBSubnetGroupName"
    }
    hidden subnetIds: DBSubnetGroupResolvable = (this) {
        property = "SubnetIds"
    }
    hidden tags: DBSubnetGroupResolvable = (this) {
        property = "Tags"
    }
}

@aws.ResourceHint {
    type = module.type
    identifier = "DBSubnetGroupName"
}
open class DBSubnetGroup extends formae.Resource {

    @aws.FieldHint
    dbSubnetGroupDescription: String?

    @aws.FieldHint{createOnly = true}
    dbSubnetGroupName: String?

    @aws.FieldHint
    subnetIds: Listing<String|formae.Resolvable>

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?

    hidden parent = this

    hidden res: DBSubnetGroupResolvable = new {
        label = parent.label
        stack = parent.stack?.label
    }
}
