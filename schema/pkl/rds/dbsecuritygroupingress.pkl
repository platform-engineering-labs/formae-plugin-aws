module aws.rds.dbsecuritygroupingress

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::RDS::DBSecurityGroupIngress"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
    discoverable = false
}
open class DBSecurityGroupIngress extends formae.Resource {

    @aws.FieldHint{
        outputField = "CIDRIP"
    }
    cidrIP: String?

    @aws.FieldHint{
        outputField = "DBSecurityGroupName"
    }
    dbSecurityGroupName: String

    @aws.FieldHint{
        outputField = "EC2SecurityGroupId"
    }
    ec2SecurityGroupId: (String|formae.Resolvable)?

    @aws.FieldHint{
        outputField = "EC2SecurityGroupName"
    }
    ec2SecurityGroupName: (String|formae.Resolvable)?

    @aws.FieldHint{
        outputField = "EC2SecurityGroupOwnerId"
    }
    ec2SecurityGroupOwnerId: (String|formae.Resolvable)?
}
