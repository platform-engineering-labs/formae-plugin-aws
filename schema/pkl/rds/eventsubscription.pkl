module aws.rds.eventsubscription

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::RDS::EventSubscription"

@aws.ResourceHint {
    type = module.type
    identifier = "SubscriptionName"
}
open class EventSubscription extends formae.Resource {

    @aws.FieldHint
    enabled: Boolean?

    @aws.FieldHint
    eventCategories: Listing<String>?

    @aws.FieldHint{createOnly = true}
    snsTopicArn: String

    @aws.FieldHint
    sourceIds: (Listing<String|formae.Resolvable>)?

    @aws.FieldHint
    sourceType: String?

    @aws.FieldHint{createOnly = true}
    subscriptionName: String?

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?
}
