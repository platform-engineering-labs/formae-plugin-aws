module aws.ec2.instanceconnectendpoint

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::InstanceConnectEndpoint"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
}
open class InstanceConnectEndpoint extends formae.Resource {

    @aws.FieldHint{
        createOnly = true
        writeOnly = true
    }
    clientToken: String?

    @aws.FieldHint{createOnly = true}
    preserveClientIp: (Boolean|formae.Resolvable)?

    @aws.FieldHint{createOnly = true}
    securityGroupIds: Listing<String|formae.Resolvable>?

    @aws.FieldHint{createOnly = true}
    subnetId: String|formae.Resolvable

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?
}
