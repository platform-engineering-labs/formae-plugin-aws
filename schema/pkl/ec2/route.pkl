module aws.ec2.vpc

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::Route"

@aws.ResourceHint {
    type = module.type
    identifier = "Ref"
    discoverable = false
}
open class Route extends formae.Resource {

    @aws.FieldHint
    carrierGatewayId: String?

    @aws.FieldHint
    coreNetworkArn: String?

    @aws.FieldHint{
        createOnly = true
    }
    destinationCidrBlock: String?

    @aws.FieldHint{
        createOnly = true
    }
    destinationIpv6CidrBlock: String?

    @aws.FieldHint{
        createOnly = true
    }
    destinationPrefixListId: String?

    @aws.FieldHint{}
    egressOnlyInternetGatewayId: String?

    @aws.FieldHint{}
    gatewayId: (String|formae.Resolvable)?

    @aws.FieldHint{}
    instanceId: (String|formae.Resolvable)?

    @aws.FieldHint{}
    localGatewayId: (String|formae.Resolvable)?

    @aws.FieldHint{}
    natGatewayId: (String|formae.Resolvable)?

    @aws.FieldHint{}
    networkInterfaceId: (String|formae.Resolvable)?

    @aws.FieldHint{
        createOnly = true
    }
    routeTableId: (String|formae.Resolvable)

    @aws.FieldHint{}
    transitGatewayId: String?

    @aws.FieldHint{}
    vpcEndpointId: String?

    @aws.FieldHint{}
    vpcPeeringConnectionId: String?

}