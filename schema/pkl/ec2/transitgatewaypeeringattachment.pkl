module aws.ec2.transitgatewaypeeringattachment

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::TransitGatewayPeeringAttachment"

@aws.ResourceHint {
    type = module.type
    identifier = "TransitGatewayAttachmentId"
}
open class TransitGatewayPeeringAttachment extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    peerAccountId: String

    @aws.FieldHint{createOnly = true}
    peerRegion: aws.Region

    @aws.FieldHint{createOnly = true}
    peerTransitGatewayId: String

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?

    @aws.FieldHint{createOnly = true}
    transitGatewayId: String
}
