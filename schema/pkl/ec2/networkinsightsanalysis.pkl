module aws.ec2.networkinsightsanalysis

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::NetworkInsightsAnalysis"

@aws.ResourceHint {
    type = module.type
    identifier = "NetworkInsightsAnalysisId"
}
open class NetworkInsightsAnalysis extends formae.Resource {

    @aws.FieldHint
    additionalAccounts: Listing<String>?

    @aws.FieldHint{createOnly = true}
    filterInArns: Listing<String>?

    @aws.FieldHint{createOnly = true}
    networkInsightsPathId: String

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?
}
