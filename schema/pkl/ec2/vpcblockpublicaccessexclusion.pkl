module aws.ec2.vpcblockpublicaccessexclusion

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::VPCBlockPublicAccessExclusion"

typealias InternetGatewayExclusionMode = "allow-bidirectional"|"allow-egress"

@aws.ResourceHint {
    type = module.type
    identifier = "ExclusionId"
}
open class VPCBlockPublicAccessExclusion extends formae.Resource {

    @aws.FieldHint
    internetGatewayExclusionMode: InternetGatewayExclusionMode

    @aws.FieldHint{createOnly = true}
    subnetId: (String|formae.Resolvable)?

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?

    @aws.FieldHint{createOnly = true}
    vpcId: (String|formae.Resolvable)?
}
