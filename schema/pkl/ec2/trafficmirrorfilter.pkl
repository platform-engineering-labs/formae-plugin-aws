module aws.ec2.trafficmirrorfilter

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::TrafficMirrorFilter"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
}
open class TrafficMirrorFilter extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    description: String?

    @aws.FieldHint
    networkServices: Listing<String>?

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?
}