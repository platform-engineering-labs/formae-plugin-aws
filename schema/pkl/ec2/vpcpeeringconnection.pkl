module aws.ec2.vpcpeeringconnection

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::EC2::VPCPeeringConnection"

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
}
open class VPCPeeringConnection extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    peerOwnerId: String?

    @aws.FieldHint{createOnly = true}
    peerRegion: aws.Region?

    @aws.FieldHint{
        createOnly = true
        writeOnly = true
    }
    peerRoleArn: String?

    @aws.FieldHint{createOnly = true}
    peerVpcId: String

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?

    @aws.FieldHint{createOnly = true}
    vpcId: String
}