module aws.route53.cidrcollection

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::Route53::CidrCollection"


@aws.SubResourceHint
open class Location extends formae.SubResource {
    cidrList: Listing<String>
    locationName: String
}

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
}
open class CidrCollection extends formae.Resource {

    @aws.FieldHint
    locations: (Listing<Location>)?

    @aws.FieldHint{createOnly = true}
    name: String(matches(Regex(#"^[0-9A-Za-z_\-]+$"#)))
}
