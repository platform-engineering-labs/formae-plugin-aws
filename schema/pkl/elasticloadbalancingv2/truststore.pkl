module aws.elasticloadbalancingv2.truststore

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::ElasticLoadBalancingV2::TrustStore"

@aws.ResourceHint {
    type = module.type
    identifier = "TrustStoreArn"
}
open class TrustStore extends formae.Resource {

    @aws.FieldHint{writeOnly = true}
    caCertificatesBundleS3Bucket: String?

    @aws.FieldHint{writeOnly = true}
    caCertificatesBundleS3Key: String?

    @aws.FieldHint{writeOnly = true}
    caCertificatesBundleS3ObjectVersion: String?

    @aws.FieldHint{createOnly = true}
    name: String?

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?
}
