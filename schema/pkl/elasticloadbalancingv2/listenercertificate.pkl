module aws.elasticloadbalancingv2.listenercertificate

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::ElasticLoadBalancingV2::ListenerCertificate"

@aws.SubResourceHint
open class Certificate extends formae.SubResource {
    certificateArn: (String|formae.Resolvable)?
}

@aws.ResourceHint {
    type = module.type
    identifier = "Id"
    discoverable = false
}
open class ListenerCertificate extends formae.Resource {

    @aws.FieldHint
    certificates: Listing<Certificate>

    @aws.FieldHint{createOnly = true}
    listenerArn: String|formae.Resolvable
}