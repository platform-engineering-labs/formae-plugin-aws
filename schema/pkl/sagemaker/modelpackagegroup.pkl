module aws.sagemaker.modelpackagegroup

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::SageMaker::ModelPackageGroup"


open class ModelPackageGroupResolvable extends formae.Resolvable {
    hidden type = module.type

    hidden modelPackageGroupArn: ModelPackageGroupResolvable = (this) {
        property = "ModelPackageGroupArn"
    }

    hidden modelPackageGroupName: ModelPackageGroupResolvable = (this) {
        property = "ModelPackageGroupName"
    }
}


@aws.ResourceHint {
    type = module.type
    identifier = "Ref"
    discoverable = false // Can be discovered but it shows weird behavior on changing the tag
}
open class ModelPackageGroup extends formae.Resource {

    @aws.FieldHint
    modelPackageGroupDescription: String?

    @aws.FieldHint{createOnly = true}
    modelPackageGroupName: String

    @aws.FieldHint
    modelPackageGroupPolicy: Dynamic?

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?

    hidden parent = this

    hidden res: ModelPackageGroupResolvable = new {
        label = parent.label
        stack = parent.stack?.label
    }
}