module aws.apprunner.vpcconnector

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::AppRunner::VpcConnector"

open class VpcConnectorResolvable extends formae.Resolvable {
    hidden type = module.type

    hidden arn: VpcConnectorResolvable = (this) {
        property = "VpcConnectorArn"
    }

    hidden revision: VpcConnectorResolvable = (this) {
        property = "VpcConnectorRevision"
    }
}

@aws.ResourceHint {
    type = module.type
    identifier = "VpcConnectorArn"
}
open class VpcConnector extends formae.Resource {

    @aws.FieldHint
    securityGroups: Listing<String|formae.Resolvable>?

    @aws.FieldHint{required = true}
    subnets: Listing<String|formae.Resolvable>

    @aws.FieldHint {
        updateMethod = "EntitySet"
        indexField = "Key"
    }
    tags: Listing<aws.Tag>?

    @aws.FieldHint{createOnly = true}
    vpcConnectorName: String?

    local parent = this

    hidden res: VpcConnectorResolvable = new {
        label = parent.label
        stack = parent.stack?.label
    }
}
