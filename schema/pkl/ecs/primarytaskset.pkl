module aws.ecs.primarytaskset

import "@formae/formae.pkl"
import "../aws.pkl"

const type = "AWS::ECS::PrimaryTaskSet"

@aws.ResourceHint {
    type = module.type
    identifier = "Cluster"
    discoverable = false
}
open class PrimaryTaskSet extends formae.Resource {

    @aws.FieldHint{createOnly = true}
    cluster: String|formae.Resolvable

    @aws.FieldHint{createOnly = true}
    service: String|formae.Resolvable

    @aws.FieldHint
    taskSetId: String|formae.Resolvable
}
